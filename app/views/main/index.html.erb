<div class="contact" id="contact-scroll">
    <h3>Or... drop me a line</h3>
    <br /><br />
    <div class="boom-box col-md-6 col-md-offset-3">
    <div class="col-md-10 col-md-offset-1">
      <form>
        <div class="form-group">
          <label for="exampleInputName">Name</label>
          <input type="name" class="form-control" id="exampleInputName" placeholder="Peter Parker">
        </div>
        <div class="form-group">
          <label for="exampleInputEmail1">Email</label>
          <input type="email" class="form-control" id="exampleInputEmail1" placeholder="pparker@dailybugle.com">
        </div>
        <div class="form-group">
          <label for="exampleInputMessage">Message</label>
          <input type="message" class="form-control" id="exampleInputMessage" placeholder="Spidey pics coming your way!">
        </div>
        <br />
        <button type="submit" class="btn btn-info">SEND!</button>
        <br />
      </form>
    </div>
  </div>
</div>

